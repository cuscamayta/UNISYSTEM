    <!-- Modal -->
    <div class="modal fade" id="modalResource" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Nuevo recurso</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label for="resource-title">Titulo * </label>
                        <input type="text" class="form-control" id="resource-title" aria-describedby="resource-title" placeholder="Titulo">
                    </div>
                    <div class="form-group">
                        <label for="resource-autor">Autor * </label>
                        <input type="text" class="form-control" id="resource-autor" aria-describedby="resource-autor" placeholder="Autor">
                    </div>
                    <div class="form-group">
                        <label for="type-resource">Tipo de Recurso *</label>
                        <select class="form-control" id="type-resource">
                            <option>Libro</option>
                            <option>Video</option>
                            <option>Audio</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="resource-description">Resumen del recurso</label>
                        <textarea class="form-control" id="resource-description" rows="3"></textarea>
                    </div>
                    <div class="form-group">
                        <label for="resource-link">Link descarga *</label>
                        <input type="text" class="form-control" id="resource-link" aria-describedby="resource-link" placeholder="Link de descarga">
                    </div>
                    <div class="form-group">
                        <label for="resource-code">Codigo de miembro *</label>
                        <input type="text" class="form-control" id="resource-code" aria-describedby="resource-code" placeholder="Codigo de Miembro">
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default " data-dismiss="modal">Cerrar</button>
                    <button type="button" id="btn-save-resource" class="btn btn-primary btn-yellow">Guardar</button>
                </div>
            </div>
        </div>
    </div>
    
    <div class="modal fade" id="modalNew" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
